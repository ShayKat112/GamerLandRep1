<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/gamerlandicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/gamerlandicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.GamerLand"
        tools:targetApi="31" >
        <activity
            android:name=".activity_welcome"
            android:exported="false" />
        <activity
            android:name=".activity_home"
            android:exported="false" />
        <activity
            android:name=".activity_register"
            android:exported="false" />
        <activity
            android:name=".activity_open_screen"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

        <uses-permission android:name="android.permission.POST_NOTIFICATIONS"/>
        <uses-permission android:name="android.permission.WRITE_CALENDAR" />
        <uses-permission android:name="android.permission.READ_CALENDAR"/>
        <uses-permission android:name="android.permission.SCHEDULE_EXACT_ALARM"/>

</manifest>